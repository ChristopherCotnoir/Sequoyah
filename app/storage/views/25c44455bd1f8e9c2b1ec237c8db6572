	<?php echo $__env->make('sections.footer', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>
</body>
<?php $__env->stopSection(); ?>
<?php echo $__env->make('page', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>